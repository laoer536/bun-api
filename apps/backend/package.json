{
  "name": "bun-api",
  "version": "1.0.50",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --target bun --outdir ./dist",
    "build:docker": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server ./src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "prisma:new": "prisma migrate dev && prisma generate",
    "prisma:pull": "prisma db pull && prisma generate",
    "prisma:push": "prisma db push && prisma generate",
    "prisma:migrate": "prisma migrate dev --name",
    "prisma:deploy": "prisma migrate deploy && prisma generate",
    "prisma:generate": "prisma generate",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.4.1",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@prisma/client": "^6.19.0",
    "elysia": "^1.4.15",
    "ioredis": "^5.8.2",
    "nodemailer": "^7.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/nodemailer": "^7.0.3",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prisma": "^6.19.0",
    "typescript-eslint": "^8.46.3"
  }
}
